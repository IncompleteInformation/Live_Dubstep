midi.DefaultChannel = 1;
midi.DeviceOut=2;

if (keyboard.A) then begin{
   midi.cc10 = var.temp
   midi.cc20 = var.temp+.0001
   var.temp=var.temp+.0001
   midi.c4= True
   wait 50 ms
}

if (keyboard.S) then begin{
   midi.cc10 = 1
   var.temp3=4
}
if (keyboard.D) then begin{
   midi.cc20 = 1
   var.temp4=5
}

